{
  "name" : "Intelligent Browsing",
  "version" : "1.0",
  "description" : "Learn better",
  "permissions" : ["activeTab", "bookmarks", "declarativeContent", "storage", "tabs", "http://*/"],
  "background" : {
    "scripts" : ["background.js", "main.js"],
    "persistent" : false
  },
  "page_action" : {
    "default_popup" : "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "login.html"
  },
  "manifest_version" : 2,
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://cdn.firebase.com https://ajax.googleapis.com https://unpkg.com; object-src 'self'"

}